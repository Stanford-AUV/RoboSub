<?xml version="1.0" ?>
<sdf version="1.6">
    <!-- See https://github.com/gazebosim/harmonic_demo/blob/8954ffb15e0ac7ffafe164889f273d94e7f6cb40/harmonic_demo/Tethys%20Sensors/model.sdf for examples of an AUV model with sensors -->
    <model name="auv">

        <link name='base_link'>
            <pose>0 0 0 0 0 0</pose>
            <inertial>
                <mass>22</mass>
                <pose>0 0 0.1 0 0 0</pose>
                <inertia>
                    <ixx>0.467</ixx>
                    <ixy>0.000000</ixy>
                    <ixz>0.000000</ixz>
                    <iyy>0.698</iyy>
                    <iyz>0.000000</iyz>
                    <izz>0.327</izz>
                </inertia>
            </inertial>
            <visual name='base_visual'>
                <geometry>
                    <box>
                        <size>0.3 0.3 0.25</size>
                    </box>
                </geometry>
            </visual>
            <collision name='base_collision'>
                <geometry>
                    <box>
                        <size>0.3 0.3 0.29</size>
                    </box>
                </geometry>
            </collision>
            <sensor name="imu_sensor" type="imu">
                <always_on>1</always_on>
                <update_rate>1</update_rate>
                <visualize>true</visualize>
                <topic>imu</topic>
            </sensor>
            <sensor name="depth_sensor" type="altimeter">
                <always_on>1</always_on>
                <update_rate>1</update_rate>
                <visualize>true</visualize>
                <topic>depth</topic>
            </sensor>
            <sensor name="dvl_sensor" type="custom" gz:type="dvl">
                <!-- Account for DVL mounting position and base link rotation -->
                <pose degrees="true">0 0 -1 0 0 180</pose>
                <always_on>1</always_on>
                <update_rate>1</update_rate>
                <topic>dvl</topic>
                <gz:dvl>
                    <type>phased_array</type>
                    <arrangement degrees="true">
                        <beam id="1">
                            <aperture>2.1</aperture>
                            <rotation>45</rotation>
                            <tilt>30</tilt>
                        </beam>
                        <beam>
                            <aperture>2.1</aperture>
                            <rotation>135</rotation>
                            <tilt>30</tilt>
                        </beam>
                        <beam>
                            <aperture>2.1</aperture>
                            <rotation>-45</rotation>
                            <tilt>30</tilt>
                        </beam>
                        <beam>
                            <aperture>2.1</aperture>
                            <rotation>-135</rotation>
                            <tilt>30</tilt>
                        </beam>
                    </arrangement>
                    <tracking>
                        <bottom_mode>
                            <when>best</when>
                            <noise type="gaussian">
                                <!-- +/- 0.4 cm/s precision at 10 m/s within 2 stddevs -->
                                <!-- <stddev>0.002</stddev> -->
                            </noise>
                            <visualize>false</visualize>
                        </bottom_mode>
                    </tracking>
                    <!-- Roughly 1 m resolution at a 100m -->
                    <resolution>0.01</resolution>
                    <!-- Approximate maximum range @ ~14.4v -->
                    <maximum_range>80.</maximum_range>
                    <minimum_range>0.1</minimum_range>
                    <!-- Preserve ENU Reference Frame (Same as Wayfinder DVL) -->
                    <reference_frame>0 0 0 0 0 0</reference_frame>
                </gz:dvl>
            </sensor>
        </link>

        <!-- Thruster Links -->
        <link name='box_1'>
            <pose>-0.272 -0.22 -0.14 0 3.1415926536 0</pose>            <!-- Position adjustment needed -->
            <visual name="visual_side_1">
                <geometry>
                    <box>
                        <size>0.2 0.2 0.01</size>
                    </box>
                </geometry>
                <material>
